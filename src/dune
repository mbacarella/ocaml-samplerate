(library
 (name samplerate)
 (public_name samplerate)
 (synopsis "OCaml bindings for samplerate")
 (foreign_stubs
  (language c)
  (names samplerate_stubs)
  (flags
   (:include c_flags.sexp)))
 (c_library_flags
  (:include c_library_flags.sexp)))

(rule
 (targets c_flags.sexp c_library_flags.sexp)
 (action
  (run ./config/discover.exe)))
